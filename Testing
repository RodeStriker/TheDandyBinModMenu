local Slider = ClientTab:Slider({
    Title = "WalkSpeed",
    Step = 1,
    Value = {
        Min = 10,
        Max = 30,
        Default = 15,
    },
    Callback = function(value)
       while wait() do
          hum.WalkSpeed = value
       end
    end
})

local Slider = ClientTab:Slider({
    Title = "Field Of View",
    Step = 1,
    Value = {
        Min = 30,
        Max = 200,
        Default = 70,
    },
    Callback = function(value)
        game.Workspace.Camera.FieldOfView = value
    end
})

local Button = ClientTab:Button({
    Title = "Kill Yourself",
    Desc = "You should kill yourself NOW",
    Callback = function()
        local Notification
        Notification = WindUI:Notify({
            Title = "Kill yourself Right NOW!",
            Content = "Would you like to kill yourself?",
            CanClose = false, -- dont allow to close the notification
            --Duration = 5, -- removing duration
            Callback = function(Button)  -- Callback
                if Button == "Confirm" then
                    game.Players.LocalPlayer.Character.Humanoid.Health = 0
                else
                    print("Canceled..")
                end
                -- Closing Notification
                Notification:Close()
            end
        })
    end,
})

