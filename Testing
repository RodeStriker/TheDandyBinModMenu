local function findItems()
    local room = workspace:FindFirstChild("CurrentRoom")
    if room then
        local floor = room:FindFirstChildWhichIsA("Model")
        if floor then
            local itemsFolder = floor:FindFirstChild("Items")
            if itemsFolder then
                for _, items in ipairs(itemsFolder:GetChildren()) do
                    if items:IsA("Model") then
                        createESP(items)
                    end
                end
            else
                warn("Items folder not found.")
            end
        else
            warn("Floor (Model) not found.")
        end
    else
        warn("CurrentRoom not found.")
    end
end

local function waitForRoom()
    repeat
        wait(1)
    until workspace:FindFirstChild("CurrentRoom")
    findItems()
end

while true do
    waitForRoom()
    local room = workspace:FindFirstChild("CurrentRoom")
    if room then
        room.Destroying:Connect(function()
            waitForRoom()
        end)
    end
    wait(5)
end
   end,
