local LGESPButton = ClientTab:Button({
    Title = "Lazy Generator ESP",
    Desc = "Must re-activate every floor",
    Callback = function()
   	local function createESP(model)
    local highlight = Instance.new("Highlight")
    highlight.Parent = model
    highlight.Adornee = model
    highlight.FillColor = Color3.new(0, 1, 0)
    highlight.OutlineColor = Color3.new(1, 1, 1)
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
end

local function findGenerators()
    local room = workspace:FindFirstChild("CurrentRoom")
    if room then
        local floor = room:FindFirstChildWhichIsA("Model")
        if floor then
            local generatorsFolder = floor:FindFirstChild("Generators")
            if generatorsFolder then
                for _, Generator in ipairs(generatorsFolder:GetChildren()) do
                    if Generator:IsA("Model") then
                        createESP(Generator)
                    end
                end
            else
                warn("Generators folder not found.")
            end
        else
            warn("Floor (Model) not found.")
        end
    else
        warn("CurrentRoom not found.")
    end
end

local function waitForRoom()
    repeat
        wait(1)
    until workspace:FindFirstChild("CurrentRoom")
    findGenerators()
end

while true do
    waitForRoom()
    local room = workspace:FindFirstChild("CurrentRoom")
    if room then
        room.Destroying:Connect(function()
            waitForRoom()
        end)
    end
    wait(5)
end
    end,
})
