    end,
})

ClientTab:Section({ Title = "Auto Skillcheck" })

local SMPosButton = ClientTab:Button({
    Title = "Adjust Mouse Position",        -- Button Title       
    Desc = "Adjust the mouse position manually",         -- Button Description 
    Callback = function()         -- Button Callback    
        isSettingClickPos = true
        WindUI:Notify({
    Title = "Set Click Position",     -- Notification Title
    Content = "Click where your Press to Skillcheck button is located at", -- Notification Content
            Duration = 5,               -- Notification Duration
        })
    end,
})

local ASCToggle = ClientTab:Toggle({
    Title = "Auto Skillcheck",            -- Toggle Title
    Title = "Hit all golds",       -- Toggle Description
    Callback = function(state)   -- Toggle Callback
        if state then
            isScriptActive = true
            WindUI:Notify({
    Title = "Auto SkillCheck",     -- Notification Title
    Content = "Enabled!", -- Notification Content
            Duration = 5,               -- Notification Duration
        })
        else
            isScriptActive = false
            WindUI:Notify({
    Title = "Auto SkillCheck",     -- Notification Title
    Content = "Disabled!", -- Notification Content
            Duration = 5,               -- Notification Duration
        })
        end
    end,
})

userInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if isSettingClickPos and not gameProcessedEvent then
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            local mousePos = userInputService:GetMouseLocation()
            clickX, clickY = mousePos.X, mousePos.Y
            isSettingClickPos = false
            WindUI:Notify({
    Title = "Set Click Position",     -- Notification Title
    Content = "Click Position has been configured!", -- Notification Content
            Duration = 5,               -- Notification Duration
        })
        end
    end
end)

runService.Heartbeat:Connect(function(deltaTime)
    if isScriptActive then
        timeElapsed = timeElapsed + deltaTime
        if timeElapsed >= checkInterval then
            checkFramesPosition()
            timeElapsed = 0
        end
    end
end)

ClientTab:Section({ Title = "ESP" })

local LMESPButton = ClientTab:Button({
    Title = "Lazy Monster ESP",
    Desc = "Must re-activate every floor",
    Callback = function()
       local function createESP(model)
    local highlight = Instance.new("Highlight")
    highlight.Parent = model
    highlight.Adornee = model
    highlight.FillColor = Color3.new(1, 0, 0)
    highlight.OutlineColor = Color3.new(1, 1, 1)
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
end

local function findMonsters()
    local room = workspace:FindFirstChild("CurrentRoom")
    if room then
        local floor = room:FindFirstChildWhichIsA("Model")
        if floor then
            local monstersFolder = floor:FindFirstChild("Monsters")
            if monstersFolder then
                for _, monster in ipairs(monstersFolder:GetChildren()) do
                    if monster:IsA("Model") then
                        createESP(monster)
                    end
                end
            else
                warn("Monsters folder not found.")
            end
        else
            warn("Floor (Model) not found.")
        end
    else
        warn("CurrentRoom not found.")
    end
end

local function waitForRoom()
    repeat
        wait(1)
    until workspace:FindFirstChild("CurrentRoom")
    findMonsters()
end

while true do
    waitForRoom()
    local room = workspace:FindFirstChild("CurrentRoom")
    if room then
        room.Destroying:Connect(function()
            waitForRoom()
        end)
    end
    wait(5)
end
   end,
})

local LIESPButton = ClientTab:Button({
    Title = "Lazy Item ESP",
    Desc = "Must re-activate every floor",
    Callback = function()
       local function createESP(model)
    local highlight = Instance.new("Highlight")
    highlight.Parent = model
