if game.PlaceId == 16116270224 then

-- Variables

--Player Variables
local plr = game.Players.LocalPlayer
local char = plr.Character
local hum = char:WaitForChild("Humanoid")

-- ScreenGui Variables
local JSGui = Instance.new("ScreenGui")
JSGui.Parent = plr.PlayerGui
JSGui.Name = "JumpscareGui"

local IL = Instance.new("ImageLabel", JSGui)
IL.Name = "Jumpscare"
IL.AnchorPoint = Vector2.new(0.5,0.5)
IL.Size = UDim2.new(0,828,0,695)
IL.Position = UDim2.new(.5,0,.5,0)
IL.BackgroundTransparency = 1
IL.Visible = false

local ESoundId = "rbxassetid://8226406520"
local Sound = Instance.new("Sound", IL)
Sound.SoundId = ESoundId
    
--Ui Lib

local WindUI = loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Footagesus/WindUI/refs/heads/main/dist/main.lua"))()

local Window = WindUI:CreateWindow({
    Title = "The Dandy Bin ModMenu", -- UI Title
    Icon = "rbxassetid://120974435696511", -- Url or rbxassetid or lucide
    Author = "By Rode Striker", -- Author & Creator
    Folder = "TDBMM", -- Folder name for saving data
    Size = UDim2.fromOffset(999,999), -- UI Size
    Transparent = true,-- UI Transparency
    Theme = "Dark", -- UI Theme
    SideBarWidth = 170, -- UI Sidebar Width (number)
})

-- Tabs

--- Section for Tabs

local ClientTab = Window:Tab({
    Title = "Client",
    Icon = "laptop",
})

local TrollTab = Window:Tab({
    Title = "Troll",
    Icon = "laugh",
})

local ATab = Window:Tab({
    Title = "Animation Packs",
    Icon = "user-round",
})

local CATab = Window:Tab({
    Title = "Custom Animation Packs",
    Icon = "user-round-pen",
})

local DividerTab = Window:Tab({
    Title = "---------",
})

local WindowTab = Window:Tab({
    Title = "Window Configuration",
    Icon = "settings",
})

local ConfigTab = Window:Tab({
    Title = "Configurations",
    Icon = "folder",
})


-- Client Tab

ClientTab:Section({ Title = "Player Settings" })

local Slider = ClientTab:Slider({
    Title = "WalkSpeed",
    Step = 1,
    Value = {
        Min = 10,
        Max = 100,
        Default = 15,
    },
    Callback = function(value)
       while wait() do
          hum.WalkSpeed = value
       end
    end
})

local Slider = ClientTab:Slider({
    Title = "Field Of View",
    Step = 1,
    Value = {
        Min = 30,
        Max = 200,
        Default = 70,
    },
    Callback = function(value)
        game.Workspace.Camera.FieldOfView = value
    end
})

ClientTab:Section({ Title = "Jumping" })

local Slider = ClientTab:Slider({
    Title = "Jump Height",
    Step = 1,
    Value = {
        Min = 0,
        Max = 30,
        Default = 0,
    },
    Callback = function(value)
        hum.JumpHeight = value
    end
})

local JBToggle = ClientTab:Toggle({
    Title = "Jump Button",            -- Toggle Title
    Desc = "For Mobile users",       -- Toggle Description
    Callback = function(state)   -- Toggle Callback
        local JB = plr.PlayerGui.TouchGui.TouchControlFrame.JumpButton
        JB.Visible = state
    end,
})

-- Troll Tab

TrollTab:Section({ Title = "Jumpscare Yourself" })

local SJSButton = TrollTab:Button({
    Title = "Very VERY Freaky Jumpscare",
    Callback = function()
        local Notification
        Notification = WindUI:Notify({
            Title = "Very Freaky Image",
            Content = "Would you like to see freaky Image?",
            CanClose = false, -- dont allow to close the notification
            --Duration = 5, -- removing duration
            Callback = function(Button)  -- Callback
                if Button == "Confirm" then
                    local Notification
        Notification = WindUI:Notify({
            Title = "VERY FREAKY IMAGE",
            Content = "Are you SURE?",
            CanClose = false, -- dont allow to close the notification
            --Duration = 5, -- removing duration
            Callback = function(Button)  -- Callback
                if Button == "Confirm" then
                    local Notification
        Notification = WindUI:Notify({
            Title = "REALLY FREAKY IMAGE",
            Content = "ARE YOU REALLY SURE?",
            CanClose = false, -- dont allow to close the notification
            --Duration = 5, -- removing duration
            Callback = function(Button)  -- Callback
                if Button == "Confirm" then
                    local Notification
        Notification = WindUI:Notify({
            Title = "SO FREAKY ROBLOX WILL BAN ME",
            Content = "ARE YOU REALLY SUREE????",
            CanClose = false, -- dont allow to close the notification
            --Duration = 5, -- removing duration
            Callback = function(Button)  -- Callback
                if Button == "Confirm" then
                    game.Players.LocalPlayer.Character.Humanoid.Health = 0
                    IL.Image = "rbxassetid://121233878354865"
                    Sound:Play()
                    IL.Visible = true
                    task.wait(4)
                    IL.Visible = false
                else
                    print("Canceled..")
                end
                -- Closing Notification
                Notification:Close()
            end
        })
                else
                    print("Canceled..")
                end
                -- Closing Notification
                Notification:Close()
            end
        })
                else
                    print("Canceled..")
                end
                -- Closing Notification
                Notification:Close()
            end
        })
                else
                    print("Canceled..")
                end
                -- Closing Notification
                Notification:Close()
            end
        })
    end,
})

local SJSButton = TrollTab:Button({
    Title = "Sprout Jumpscare",
    Callback = function()
        IL.Image = "rbxassetid://120974435696511"
        Sound:Play()
        IL.Visible = true
        task.wait(5)
        IL.Visible = false
    end,
})

